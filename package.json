{
  "name": "automacao-mensagem-espera",
  "version": "1.0.0",
  "description": "Sistema de automação para envio de mensagens de espera integrado com CAM Krolik",
  "main": "src/cedirp.js",
  "scripts": {
    "build": "echo 'No build needed for JavaScript'",
    "start": "node src/cedirp.js",
    "start:prod": "NODE_ENV=production node src/cedirp.js",
    "dev": "nodemon src/cedirp.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:production": "node scripts/test-production-flow.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "clean": "rimraf dist",
    "setup": "chmod +x scripts/*.sh && ./scripts/setup.sh",
    "setup:production": "node scripts/setup-production.js",
    "health-check": "./scripts/health-check.sh",
    "logs": "tail -f logs/app.log",
    "backup": "node scripts/backup.js",
    "start:production": "pm2 start ecosystem.config.js --env production",
    "stop:production": "pm2 stop ecosystem.config.js",
    "restart:production": "pm2 restart ecosystem.config.js",
    "logs:production": "pm2 logs",
    "monitor:production": "pm2 monit",
    "health:check": "curl -f http://localhost:8080/health",
    "metrics:check": "curl -f http://localhost:9090/metrics",
    "validate:supabase": "node scripts/validate-supabase.js",
    "test:chat-list": "node scripts/test-chat-list.js",
    "test:action-cards": "node scripts/test-action-cards.js",
    "example:action-cards": "node examples/action-cards-example.js"
  },
  "keywords": [
    "automation",
    "messaging",
    "cam-krolik",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "luxon": "^3.7.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.1"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5"
  }
}
